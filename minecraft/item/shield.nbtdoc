use ::minecraft::util::color::ColorInt;
use ::minecraft::util::BannerPattern;

compound Shield extends super::ItemBase {
	/// The tag for the shield
	BlockEntityTag: Tag
}

compound Tag {
	/// The patterns on the shield
	Patterns: [Pattern]
}

compound Pattern {
	/// The color of the pattern
	Color: ColorInt,
	/// The pattern id
	Pattern: BannerPattern
}