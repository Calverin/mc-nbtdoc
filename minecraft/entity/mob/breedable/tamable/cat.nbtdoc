use ::minecraft::util::Color;

compound Cat extends super::Tamable {
	/// The type of the cat
	CatType: CatType,
	/// The cats collar color, present for stray cats
	CollarColor: Color
}

/// The type of the cat
enum(int) CatType {
	Invisible = -1,
	Tabby = 0,
	Tuxedo = 1,
	Red = 2,
	Siamese = 3,
	Shorthair = 4,
	Calico = 5,
	Persain = 6,
	Ragdoll = 7,
	White = 8,
	Jellie = 9,
	Black = 10
}